{
    "version": 2,
    "builds": [
        {
          "src": "app.py",
          "use": "@vercel/python",
          "config": {
            "maxDuration": 30,
            "memory": 1024
          }
        }
      ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "app.py"
      }
    ]
  }
  
